{
  "name": "zooid-meshblu-device-editor",
  "version": "3.1.1",
  "description": "A React component for editing Meshblu devices.",
  "main": "dist/index.js",
  "scripts": {
    "bab": "babel index.js --out-dir lib",
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node dev-server.js",
    "lint": "eslint .",
    "mocha": "NODE_ENV=test mocha test/.setup.js components/**/*.spec.js",
    "test": "npm run mocha",
    "test:watch": "npm run mocha -- -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/zooid-meshblu-device-editor.git"
  },
  "author": "Octoblu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/zooid-meshblu-device-editor/issues"
  },
  "homepage": "https://github.com/octoblu/zooid-meshblu-device-editor#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.3.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "jsdom": "^8.3.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "mocha-multi": "^0.9.0",
    "mocha-osx-reporter": "^0.1.2",
    "postcss-loader": "^0.8.1",
    "postcss-scss": "^0.1.6",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.0.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  },
  "dependencies": {
    "lodash": "^4.11.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-jsonschema-form": "^0.24.0"
  }
}
