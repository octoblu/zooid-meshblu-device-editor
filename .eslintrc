{
  "extends": "airbnb",
  "rules": {
    "react/prop-types": 0,
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0,
    "arrow-body-style": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-multi-spaces": 0,
    "no-unused-expressions": 0,
    "comma-dangle": 1,
    "max-len": 0,
    "no-console": 0,
    "semi": [2, "never"],
    "no-param-reassign": 0,
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "mocha": true
  }
}
